import java.util.ArrayList;

/**
 * @(#) Table.java
 */
public class Table
{
	private Integer tableNo;
	
	private MealOrder mealOrder;
	
	private Boolean occupied;
	
	private Restaurant restaurant;
	
	ArrayList<Waiter> servingWaiters ;

	private Waiter assignedwaiter;

	public Table(Integer tableNo, MealOrder mealOrder, Boolean occupied,
			Restaurant restaurant, ArrayList<Waiter> servingWaiters,
			Waiter assignedwaiter) {
		this.tableNo = tableNo;
		this.mealOrder = mealOrder;
		this.occupied = occupied;
		this.restaurant = restaurant;
		this.servingWaiters = servingWaiters;
		this.assignedwaiter = assignedwaiter;
	}
	
	
}
